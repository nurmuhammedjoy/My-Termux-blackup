# Ignore system junk
.DS_Store
Thumbs.db
*.bak
*.swp

# Ignore Trash and temp files
.local/share/Trash/
*.log
*.tmp

# Ignore secrets and tokens
*.env
*.token
*.key
*.pem
secrets.*
config.json
credentials.json

